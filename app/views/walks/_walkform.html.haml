#walkformdiv
  = form_for @walk do |f|
    = f.label :neighborhood
    = f.text_field :neighborhood, :autofocus => true
    = f.label :date
    = f.text_field  :date
    = f.label :time
    = f.text_field :time
    = f.label :description
    = f.text_field :description
    %br
    = f.submit 'Add Walk', :class => 'btn btn-primary'

